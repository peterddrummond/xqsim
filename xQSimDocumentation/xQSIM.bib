	eprinttype = {arxiv},
	file = {C\:\\Users\\Alex\\Zotero\\storage\\RGN82XQ5\\Mart{\'\i}nez-Cifuentes et al. - 2022 - Classical models are a better explanation of the J.pdf},
	keywords = {Quantum Physics},
	langid = {english},
	month = jul,
	number = {arXiv:2207.10058},
	primaryclass = {quant-ph},
	publisher = {{arXiv}},
	title = {Classical Models Are a Better Explanation of the {{Jiuzhang Gaussian Boson Samplers}} than Their Targeted Squeezed Light Models},
	year = {2022}}

