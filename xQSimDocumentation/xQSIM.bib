@article{Afek879,
	abstract = {Entanglement, where a system can be in a superposition of a number of distinct states simultaneously, is a principle at the foundation of quantum mechanics (recall Schr{\"o}dinger{\textquoteright}s cat, which is both dead and alive). It can also be used in many applications{\textemdash}imaging, communication, patterning, and metrology{\textemdash}with the effect being amplified by entangling larger systems. However, the systematic generation of {\textquotedblleft}large{\textquotedblright} entangled systems is challenging. Afek et al. (p. 879; see the Perspective by Wildfeuer) present a technique for generating many-photon entanglement in so-called NOON states, where there are two possible paths and N photons in one path and 0 in the other{\textemdash}the system being a superposition of {\textquotedblleft}all and nothing{\textquotedblright} states. Mixing of entangled pairs with classical light at a beam splitter formed up to five photon-entangled states. The technique should be generally applicable to generate higher-order entangled states.Precision measurements can be brought to their ultimate limit by harnessing the principles of quantum mechanics. In optics, multiphoton entangled states, known as NOON states, can be used to obtain high-precision phase measurements, becoming more and more advantageous as the number of photons grows. We generated {\textquotedblleft}high-NOON{\textquotedblright} states (N = 5) by multiphoton interference of quantum down-converted light with a classical coherent state in an approach that is inherently scalable. Super-resolving phase measurements with up to five entangled photons were produced with a visibility higher than that obtainable using classical light only.},
	author = {Afek, Itai and Ambar, Oron and Silberberg, Yaron},
	date-added = {2019-07-03 08:00:31 +0000},
	date-modified = {2019-07-03 08:13:53 +0000},
	doi = {10.1126/science.1188172},
	issn = {0036-8075},
	journal = {Science},
	number = {5980},
	pages = {879--881},
	publisher = {American Association for the Advancement of Science},
	title = {High-NOON States by Mixing Quantum and Classical Light},
	url = {https://science.sciencemag.org/content/328/5980/879},
	volume = {328},
	year = {2010},
	bdsk-url-1 = {https://science.sciencemag.org/content/328/5980/879},
	bdsk-url-2 = {http://dx.doi.org/10.1126/science.1188172}}

@article{Lavoie_2009,
	abstract = {It is well known that quantum mechanics is incompatible with local realistic theories. Svetlichny showed, through the development of a Bell-like inequality, that quantum mechanics is also incompatible with a restricted class of nonlocal realistic theories for three particles where any two-body nonlocal correlations are allowed (Svetlichny 1987 Physical Review D 35 3066). In the present work, we experimentally generate three-photon GHZ states to test Svetlichny's inequality. Our states are fully characterized by quantum state tomography using an overcomplete set of measurements and have a fidelity of (84$\pm$1)% with the target state. We measure a convincing, 3.6σ, violation of Svetlichny's inequality and rule out this class of restricted nonlocal realistic models.},
	author = {J Lavoie and R Kaltenbaek and K J Resch},
	date-added = {2019-07-03 08:00:31 +0000},
