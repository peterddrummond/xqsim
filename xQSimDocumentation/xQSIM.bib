	volume = {49},
	year = {1994}}

@article{Dowling2005,
	abstract = {The calculation of quantum dynamics is currently a central issue in theoretical physics, with diverse applications ranging from ultracold atomic Bose-Einstein condensates to condensed matter, biology, and even astrophysics. Here we demonstrate a conceptually simple method of determining the regime of validity of stochastic simulations of unitary quantum dynamics by employing a time-reversal test. We apply this test to a simulation of the evolution of a quantum anharmonic oscillator with up to 6.022{\~A}1023 (Avogadro's number) of particles. This system is realizable as a Bose-Einstein condensate in an optical lattice, for which the time-reversal procedure could be implemented experimentally.},
	author = {Dowling, Mark R. and Drummond, Peter D. and Davis, Matthew J. and Deuar, Piotr},
	date-added = {2021-02-20 17:11:07 +1100},
	date-modified = {2021-02-20 17:11:07 +1100},
