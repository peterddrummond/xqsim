	volume = {50},
	year = {1983}}

@article{Drummond:1989,
	abstract = {The mean time taken for the degenerate parametric oscillator to quantum tunnel between its two above-threshold stable states is calculated in the large photon-number limit. The result is compared with an earlier approximate result using a truncated Fokker-Planck equation in the Wigner representation. This earlier theory is equivalent to stochastic electrodynamics. Our quantum theory results indicate orders of magnitude faster tunneling far above threshold, when compared to the truncated Wigner theory.},
	author = {Drummond, P. D. and Kinsler, P.},
	date-added = {2021-02-20 17:11:07 +1100},
	date-modified = {2021-02-20 17:11:07 +1100},
